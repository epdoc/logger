{
  "name": "@epdoc/logjava",
  "version": "0.0.1-alpha.2",
  "description": "Java-style logger implementation for @epdoc/logger with standard Java log levels",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "license": "MIT",
  "tasks": {
    "test": "deno test -A",
    "lint": "deno lint",
    "check": "deno check src test",
    "prepublish": "deno publish --dry-run",
    "publish": "deno publish"
  },
  "exports": "./src/mod.ts",
  "test": {
    "include": [
      "src/**/*test.ts",
      "test/**/*test.ts"
    ]
  },
  "lint": {
    "include": [
      "src",
      "test/**/*test.ts"
    ]
  },
  "fmt": {
    "include": [
      "src",
      "test/**/*test.ts",
      "deno.json"
    ],
    "singleQuote": true,
    "lineWidth": 120
  },
  "imports": {
    "@epdoc/logger": "../logger/src/mod.ts",
    "@epdoc/loglevels": "../loglevels/src/mod.ts",
    "@epdoc/msgbuilder": "../msgbuilder/src/mod.ts",
    "@std/fmt/colors": "jsr:@std/fmt@^1.0.8/colors",
    "@epdoc/type": "jsr:@epdoc/type@^1.1.13",
    "@epdoc/duration": "jsr:@epdoc/duration@^0.2.0",
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/testing": "jsr:@std/testing@^1.0.15"
  }
}