{
  "name": "@epdoc/logdy",
  "version": "0.0.1-alpha.0",
  "description": "Logdy transport for @epdoc/logger - stream logs to Logdy web interface",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "license": "MIT",
  "tasks": {
    "test": "deno test -A",
    "lint": "deno lint",
    "check": "deno check src test",
    "prepublish": "deno publish --dry-run",
    "publish": "deno publish"
  },
  "exports": "./src/mod.ts",
  "test": {
    "include": [
      "src/**/*test.ts",
      "test/**/*test.ts"
    ]
  },
  "lint": {
    "include": [
      "src",
      "test/**/*test.ts"
    ]
  },
  "fmt": {
    "include": [
      "src",
      "test/**/*test.ts",
      "deno.json"
    ],
    "singleQuote": true,
    "lineWidth": 120
  },
  "imports": {
    "@epdoc/logger": "../logger/src/mod.ts",
    "@epdoc/msgbuilder": "../msgbuilder/src/mod.ts",
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/testing": "jsr:@std/testing@^1.0.15"
  }
}
