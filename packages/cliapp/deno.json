{
  "name": "@epdoc/cliapp",
  "description": "A CLI application template for @epdoc/logger",
  "version": "1.0.7",
  "exports": "./src/mod.ts",
  "tasks": {
    "dev": "deno test --watch",
    "test": "deno test -A test/*.test.ts",
    "check": "deno check src test",
    "examples": "./examples/run.sh"
  },
  "license": "MIT",
  "imports": {
    "@epdoc/logger": "jsr:@epdoc/logger@^1003.0.6",
    "@epdoc/msgbuilder": "jsr:@epdoc/msgbuilder@^0.0.8",
    "@epdoc/type": "jsr:@epdoc/type@^1.1.20",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "commander": "npm:commander@^14.0.2"
  },
  "lint": {
    "include": [
      "src",
      "test"
    ]
  },
  "fmt": {
    "singleQuote": true,
    "lineWidth": 120,
    "include": [
      "src",
      "test"
    ]
  }
}