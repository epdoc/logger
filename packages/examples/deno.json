{
  "name": "@epdoc/logger-examples",
  "version": "0.0.1-alpha.9",
  "description": "Logging module with chainable formatting, middleware mixins and extendable transports",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "license": "MIT",
  "tasks": {
    "lint": "deno lint",
    "check": "deno check *.ts lib",
    "examples": "./run.sh"
  },
  "exports": "./simple.ts",
  "lint": {
    "include": [
      "lib/**/*.ts",
      "*.ts"
    ]
  },
  "fmt": {
    "include": [
      "lib/**/*.ts",
      "*.ts",
      "deno.json"
    ],
    "singleQuote": true,
    "lineWidth": 120
  },
  "imports": {
    "@epdoc/cliapp": "jsr:@epdoc/cliapp@^1.1.0",
    "@epdoc/logger": "jsr:@epdoc/logger@^1003.1.0",
    "@epdoc/msgbuilder": "jsr:@epdoc/msgbuilder@^0.1.0-alpha.3",
    "@epdoc/type": "jsr:@epdoc/type@^1.2.2"
  }
}