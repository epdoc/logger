{
  "name": "@epdoc/logger-examples",
  "version": "0.0.1-alpha.14",
  "description": "Logging module with chainable formatting, middleware mixins and extendable transports",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "license": "MIT",
  "tasks": {
    "lint": "deno lint",
    "check": "deno check *.ts",
    "examples": "./run.sh"
  },
  "exports": [],
  "lint": {
    "include": [
      "deno.json",
      "*.ts"
    ]
  },
  "fmt": {
    "include": [
      "*.ts",
      "deno.json"
    ],
    "singleQuote": true,
    "lineWidth": 120
  },
  "imports": {
    "@epdoc/cliapp": "file:../cliapp/src/mod.ts",
    "@epdoc/logger": "file:../logger/src/mod.ts",
    "@epdoc/loglevels": "file:../loglevels/src/mod.ts",
    "@epdoc/msgbuilder": "file:../msgbuilder/src/mod.ts",
    "@epdoc/type": "jsr:@epdoc/type@^1.2.4"
  }
}