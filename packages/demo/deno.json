{
  "name": "@epdoc/demo",
  "version": "0.0.12",
  "description": "Demo CliApp demo project",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "license": "MIT",
  "tasks": {
    "test": "deno test --allow-all",
    "lint": "deno lint *.ts *.json src test",
    "check": "deno check *.ts *.json src test",
    "prepublish": "deno fmt *.ts *.json src test && deno task lint && deno task check",
    "install": "deno install --global -A -f --config deno.json --name cliapp-demo main.ts"
  },
  "exports": "./main.ts",
  "lint": {
    "include": [
      "src",
      "test",
      "main.ts",
      "deno.json"
    ]
  },
  "fmt": {
    "include": [
      "src",
      "test",
      "main.ts",
      "deno.json"
    ],
    "singleQuote": true,
    "lineWidth": 120
  },
  "imports": {
    "@epdoc/cliapp": "file:../cliapp/src/mod.ts",
    "@epdoc/fs": "jsr:@epdoc/fs@^1.2.14",
    "@epdoc/logger": "file:../logger/src/mod.ts",
    "@epdoc/loglevels": "file:../loglevels/src/mod.ts",
    "@epdoc/msgbuilder": "file:../msgbuilder/src/mod.ts",
    "@epdoc/type": "jsr:@epdoc/type@^1.2.4",
    "@std/assert": "jsr:@std/assert@^1.0.16"
  }
}