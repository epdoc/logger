    "del": "^7.1.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "express": "^4.21.0",
    "gulp": "^5.0.0",
